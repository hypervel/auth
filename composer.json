{
    "name": "swooletw/hyperf-auth",
    "description": "The Support package for Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "auth",
        "swoole",
        "laravel"
    ],
    "support": {
        "issues": "https://github.com/swooletw/hyperf-packages/issues",
        "source": "https://github.com/swooletw/hyperf-packages"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert.cht@pm.me",
            "homepage": "https://albert-chen.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "nesbot/carbon": "^2.16",
        "hyperf/context": "~3.1.0",
        "hyperf/stringable": "~3.1.0",
        "hyperf/macroable": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/config": "~3.1.0",
        "hyperf/database": "~3.1.0",
        "hyperf/http-server": "~3.1.0",
        "swooletw/hyperf-hashing": "~3.1.0",
        "swooletw/hyperf-jwt": "~3.1.0"
    },
    "suggest": {
        "hyperf/session": "Required to use session guard. (~3.1.0)"
    },
    "autoload": {
        "psr-4": {
            "SwooleTW\\Hyperf\\Auth\\": "src/"
        },
        "files": [
            "src/Functions.php"
        ]
    },
    "extra": {
        "hyperf": {
            "config": "SwooleTW\\Hyperf\\Auth\\ConfigProvider"
        },
        "branch-alias": {
            "dev-master": "3.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
